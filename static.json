{
  "root": "build/",
  "clean_urls": true,
  "error_page": "index.html",
  "headers": {
    "/": {
      "Cache-Control": "no-cache"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  "routes": {
    "/**": "index.html"
  },
  "logging": {
    "access": false
  }
}